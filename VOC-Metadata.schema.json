{
  "title": "VOC audio metadata schema",
  "description": "VOC audio metadata schema v1",
  "type": "object",
  "properties": {
    "nombre_audio": {
      "type": "string",
	  "description": "Name of original audio file"
    },
    "id": {
      "type": "string",
	  "description": "Call ID"
    },
    "nombre_agente": {
      "type": "string",
	  "description": "Name of agent into the call"
    },
    "agentid": {
      "type": "string",
	  "description": "Id of agent into the call"
    },
    "ANI": {
      "type": "string",
	  "description": "Call ANI"
    },
    "datetime": {
      "type": "string",
	  "description": "Time when the call was placed, in format YYYY-MM-DD HH:MM:SS",
	  "minLength": 19,
	  "maxLength": 19
    },
    "record_start_time": {
      "type": "string",
	  "description": "Time when the recording call started, in format YYYY-MM-DD HH:MM:SS",
	  "minLength": 19,
	  "maxLength": 19
    },
    "record_end_time": {
      "type": "string",
	  "description": "Time when ends the recording call, in format YYYY-MM-DD HH:MM:SS",
	  "minLength": 19,
	  "maxLength": 19
    },
    "record_duracion_audio": {
      "type": "string",
	  "description": "Recording duration, in format HH:MM:SS",
	  "minLength": 8,
	  "maxLength": 8
    },
    "tipo_llamada": {
      "type": "string",
	  "description": "Call Direction"
	  "pattern": "^(In|Out)bound$"
    },
    "Custom_data_01": {
